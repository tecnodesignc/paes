{
    "name": "tecnodesignc/menu-module",
    "type": "encore-module",
    "description": "Menu module for EncoreCMS 2. Handles all the menus.",
    "keywords": [
        "encorecms",
        "menu",
        "tree",
        "nested sets"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Marcos Rativa",
            "email": "marcos21.009@gmail.com",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "support@tecnodesign.com.co",
        "issues": "https://github.com/tecnodesignc/menu-modules/issues",
        "source": "https://github.com/tecnodesignc/menu-modules"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "require": {
        "php": ">=8.0",
        "composer/installers": "^2.1",
        "tecnodesignc/laravel-menus": "^8.0",
        "typicms/nestablecollection": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "~6.0",
        "orchestra/testbench": "3.5.*",
        "fzaninotto/faker": "1.4.0",
        "doctrine/dbal": "~2.5",
        "tecnodesignc/page-module": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Modules\\Menu\\": ".",
            "Modules\\": "modules/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
